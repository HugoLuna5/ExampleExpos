<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColorSecondary">#FFF</item>

    </style>
    <style name="AppTheme.NoActionBar" >
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="ButonCreateGroup" parent="Widget.MaterialComponents.Button">
        <item name="cornerRadius">20dp</item>
        <item name="iconPadding">2dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">#546DFE</item>
    </style>
    <style name="ButonCustomNormal" parent="Widget.MaterialComponents.Button">
        <item name="iconPadding">2dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">#546DFE</item>
    </style>
    <style name="ButonCustomDanger" parent="Widget.MaterialComponents.Button">
        <item name="iconPadding">2dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">@color/md_red_500</item>
    </style>
    <style name="ButonClose" parent="Widget.MaterialComponents.Button">
        <item name="cornerRadius">20dp</item>
        <item name="iconPadding">2dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">@color/md_red_500</item>
    </style>

    <style name="ToolbarColoredBackArrow" parent="AppTheme">
    </style>
    <!--Etiquetas-->
    <style name="letra">
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">sans-serif-black</item>

    </style>
    <style name="sans_serif_black">
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">sans-serif-black</item>

    </style>
    <style name="sans_serif">
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">sans-serif</item>

    </style>
    <style name="sans_serif_medium">
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">sans-serif-medium</item>

    </style>

</resources>
